<?xml version="1.0"?>
<launch>
  <arg name="measurements_file" default="$(find rct_examples)/data/positioner_kinematic_measurements.yaml"/>
  <node name="kinematic_calibration_example" pkg="rct_examples" type="kinematic_calibration" output="screen">
    <param name="measurements_file" value="$(arg measurements_file)"/>
    <rosparam>
      camera_mount_to_camera_guess:
        x: 2.2
        y: 0.7
        z: 1.075
        qw: 1.0
        qx: 0.0
        qy: 0.0
        qz: 0.0
      target_mount_to_target_guess:
        x: 0.17
        y: -0.65
        z: 0.5
        qw: 0.547419
        qx: 0.447585
        qy: -0.547419
        qz: -0.447585
    </rosparam>
  </node>
</launch>
